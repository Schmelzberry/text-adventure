{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 1,
    // "indent": ["warn", 2, { "indentSwitchCase": false }],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn"
  }
}
